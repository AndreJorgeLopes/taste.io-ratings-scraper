TASTE_USERNAME=YOUR_USERNAME
# Your taste.io authentication token without the "Bearer " part (required for continue-watching and episode tracking)
# Normally changes every 8 days (you can check that date in the cookies field "token")
TASTE_TOKEN=YOUR_TASTE_TOKEN
# Your client_id gotten by creating a new app at https://simkl.com/settings/developer/ (Max 1000 requests per day)
SIMKL_CLIENT_ID=YOUR_CLIENT_ID
# Your Simkl access token gotten by following the instructions at this link: https://simkl.docs.apiary.io/#reference/authentication-oauth-2.0/
SIMKL_ACCESS_TOKEN=YOUR_ACCESS_TOKEN
HEADLESS_MODE=TRUE
MIN_DELAY=1.5
MAX_DELAY=4
PAGE_LOAD_TIMEOUT=30
OUTPUT_FILE=SimklBackup.json
CACHE_FILE=ratings_cache.json
CACHE_TIMEOUT_DAYS=1

# Feature toggles (all enabled by default)
SCRAPE_RATINGS=TRUE
SCRAPE_SAVED=TRUE
SCRAPE_CONTINUE_WATCHING=TRUE